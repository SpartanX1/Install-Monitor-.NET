<Window x:Class="InstallMonitor.FileMonitorView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InstallMonitor"
        mc:Ignorable="d"
        Title="FileMonitorView" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <TextBlock Margin="5,10,0,10" DockPanel.Dock="Top">File Log</TextBlock>
            <TextBox Margin="5" x:Name="FileLog" DockPanel.Dock="Bottom"></TextBox>
        </DockPanel>
    </Grid>
</Window>
